<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bank.SearchController">
   <children>
      <GridPane layoutX="-2.0" layoutY="-1.0" prefHeight="327.0" prefWidth="606.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text=" tablename" />
            <Label text=" limit" GridPane.rowIndex="1" />
            <TextField fx:id="TableName" prefHeight="40.0" prefWidth="303.0" GridPane.columnIndex="1" />
            <TextField fx:id="Limit" prefHeight="43.0" prefWidth="303.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <HBox layoutX="401.0" layoutY="363.0">
         <children>
            <Button fx:id="Ok" layoutX="401.0" layoutY="363.0" mnemonicParsing="false" onAction="#handleOk" text="子查询插入" />
            <Button fx:id="search" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#handleSearch" text="查询" />
            <Button fx:id="cancel" mnemonicParsing="false" onAction="#handleCancel" text="取消" />
         </children>
      </HBox>
   </children>
</AnchorPane>
